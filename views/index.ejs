  <script>
  var title = "<%= locals.countries %>".toLowerCase();
  var visitedCountries = title.split(",");
  var countries=[];
  var count = document.querySelectorAll("path").length; // 26
  var array = document.querySelectorAll("path"); 
  for (let index = 0; index < array.length; index++) {
     countries.push(array[index].getAttribute("title"));
  }
  
  for (let i= 0; i < visitedCountries.length; i++) {
    for (let j = 0; j < array.length; j++) {
      if(visitedCountries[i] == countries[j]){
        document.querySelectorAll("path")[j].style.fill="crimson";
      }
    }
  }
